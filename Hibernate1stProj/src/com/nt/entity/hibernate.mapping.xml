<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
      <hibernate-mapping>
     <class name="com.nt.entity.Product" table="PRODUCTS">
     <id name="pid" column="PID"/>
     <property name="pname" column="PNAME"></property>
     <property name="price" column="PRICE"></property>
     <property name="stockqty" column="STOCKQTY"></property>
     <property name="mfgdate" column="MFGDATE"></property>
     <property name="expirydate" column="EXPIRYDATE"></property>
    
     </class>
     
      
      
      
      </hibernate-mapping>